@{
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
@* @model List<KeyHouse.Models.Entities.Users> *@
@model KeyHouse.ModelView.AgencyContractViewModel
<section>
    <div class="container-xxl py-5">
        <div class="card">
            <div class="card-body">
                <div class="row justify-content-md-center">
                    <div class="col-md-8">
                        <div class="wow fadeInUp" data-wow-delay="0.5s">
                            <h1 class="text-center">Add Agency Contract </h1>
                            <form asp-controller="Admin" asp-action="SaveContract" method="post" enctype="multipart/form-data">
                                <div>
                                    @Html.HiddenFor(model => model.AgencyID)
                                </div>
                                <div class="row">
                                    <div class="mb-3">
                                        <label asp-for="Contract_Name">Contract_Name</label>
                                        <span asp-validation-for="Contract_Name" class="text-danger"></span>
                                        <div class="form-floating">
                                            <input type="text" asp-for="Contract_Name" class="form-control"  placeholder="Contract Name" />

                                        </div>
                                    </div>
                                    <div class="mb-3">
                                        <label asp-for="Start_date">Start Date</label>
                                        <span asp-validation-for="Start_date" class="text-danger"></span>
                                        <div class="form-floating">
                                            <input type="date" asp-for="Start_date" class="form-control"   />

                                        </div>
                                    </div>
                                    <div class="mb-3">
                                        <label asp-for="End_date">End Date</label>
                                        <span asp-validation-for="End_date" class="text-danger"></span>

                                        <div class="form-floating">
                                           <input type="date" asp-for="End_date" class="form-control"  />

                                        </div>
                                    </div>
                                    <div class="col-12">
                                        <button class="btn btn-primary w-100 py-3" type="submit">Submit</button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
