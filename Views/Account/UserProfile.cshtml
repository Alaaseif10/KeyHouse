@model KeyHouse.Models.Entities.Users
<section>
    <div class="container-xxl py-5">
        <div class="card mx-auto" style="width: 35rem; margin:5px;align-items:center">
            <div class="col-md-8">
                <div class="wow fadeInUp" data-wow-delay="0.5s">
                    <img src="~/img/profile-user.png" class="card-img-top" alt="...">
                </div>
                <div class="card-body">
                    <h5 class="mb-0">Full Name :  @Model.UserName</h5>
                    <br />
                    <p> <i class="bi bi-envelope-fill"></i> @Model.Email</p>
                    <p> <i class="bi bi-telephone-fill"></i> @Model.PhoneNumber</p>
                    <p> Address: Cairo, Egypt</p>
                    <h6 class="mb-0 font-weight-light">To Change Any Data Kindly Contact IT Adminstrator</h6>
                </div>
            </div>
        </div>
    </div>
</section>